<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>CertTools.CreateSigningCert</RootNamespace>
    <Title>Code Signing Certificate Creation Tool</Title>
    <AssemblyTitle>$(Title)</AssemblyTitle>
    <ApplicationIcon>CreateSigningCert.ico</ApplicationIcon>
    <OutputType>Exe</OutputType>
    <SupportedOSPlatformVersion>10.0.26100.0</SupportedOSPlatformVersion>
    <RuntimeIdentifiers>win-x64</RuntimeIdentifiers>
    <TargetFramework>net10.0-windows10.0.26100.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <InvariantGlobalization>true</InvariantGlobalization>
    <TrimMode>partial</TrimMode>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="CreateSigningCert.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CertCore\CertCore.csproj" />
  </ItemGroup>

</Project>
